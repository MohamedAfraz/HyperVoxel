			"settings": {
				"foreground": "#F1DC03"
			}
		},
		{
			"name": "[JSON] - Color for String",
			"scope": ["source.json string", "source.json punctuation.definition.string"],
			"settings": {
				"foreground": "#92FC79"
			}
